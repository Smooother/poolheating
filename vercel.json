{
  "functions": {
    "api/daily-scheduler.js": {
      "maxDuration": 60
    },
    "api/schedule-executor.js": {
      "maxDuration": 30
    },
    "api/poll-tuya-status.js": {
      "maxDuration": 30
    }
  },
  "crons": [
    {
      "path": "/api/daily-scheduler",
      "schedule": "20 13 * * *"
    },
    {
      "path": "/api/schedule-executor",
      "schedule": "0 14 * * *"
    },
    {
      "path": "/api/poll-tuya-status",
      "schedule": "*/2 * * * *"
    }
  ]
}